{
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "printWidth": 120,
    "endOfLine": "auto",
    "overrides": [
        {
            "files": ["*.ts", "*.tsx"],
            "options": {
                "parser": "typescript",
                "tabWidth": 4
            }
        }
    ]
}
